# Environment variables for Tokenize launchpad
# Copy this file to `.env` and fill in real values.

# Solana JSON-RPC endpoint (e.g. https://api.devnet.solana.com)
VITE_RPC_ENDPOINT="https://api.devnet.solana.com"

# Network label (mainnet-beta | devnet | testnet)
VITE_SOLANA_NETWORK="devnet"

# Commitment level
VITE_COMMITMENT="confirmed"

# Base58 secret key of mint authority wallet (for local/dev only!)
VITE_MINT_AUTHORITY_SECRET="[]" 